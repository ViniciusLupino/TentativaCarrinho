@model IEnumerable<EcoImpulseWebSite.Models.Carrinho>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProdutosId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProdutosId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCarrinho">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCarrinho">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCarrinho">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
